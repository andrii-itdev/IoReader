<Page x:Class="IoReader.LibraryPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:IoReader.Views"
      xmlns:viewmodels="clr-namespace:IoReader"
      xmlns:viewModels="clr-namespace:IoReader.ViewModels"
      mc:Ignorable="d" 
      d:DesignHeight="450" 
      d:DesignWidth="800"
      VerticalAlignment="Stretch"
      Background="Aqua"
      Title="LibraryPage">
    <Page.DataContext>
        <viewModels:LibraryViewModel x:Name="LibraryDataContext" />
    </Page.DataContext>

    <Grid>
        <TextBlock Text="Library"
                   HorizontalAlignment="Center"
                   VerticalAlignment="Center"
                   Height="1000" />
    </Grid>
</Page>
